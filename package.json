{
  "name": "english-speaking-training",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "db:setup": "yarn rollback:all && yarn migrate && yarn seed",
    "dev": "nodemon server",
    "migrate": "knex migrate:latest --knexfile server/knexfile.js",
    "migrate:make": "knex migrate:make --knexfile server/knexfile.js",
    "rollback": "knex migrate:rollback --knexfile server/knexfile.js",
    "rollback:all": "knex migrate:rollback --knexfile server/knexfile.js --all",
    "seed": "knex seed:run --knexfile server/knexfile.js",
    "start": "yarn rollback:all && yarn migrate && yarn seed && node server"
  },
  "dependencies": {
    "axios": "^0.19.0",
    "core-js": "^3.3.2",
    "express": "^4.17.1",
    "knex": "^0.20.1",
    "morgan": "^1.9.1",
    "pg": "^7.12.1",
    "vue": "^2.6.10",
    "vuetify": "^2.1.0",
    "vuex": "^3.1.2"
  },
  "devDependencies": {
    "@fortawesome/fontawesome-free": "^5.11.2",
    "@vue/cli-plugin-babel": "^4.0.0",
    "@vue/cli-plugin-eslint": "^4.0.0",
    "@vue/cli-plugin-unit-mocha": "^4.0.5",
    "@vue/cli-service": "^4.0.0",
    "@vue/eslint-config-prettier": "^5.0.0",
    "@vue/test-utils": "^1.0.0-beta.29",
    "babel-eslint": "^10.0.3",
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "eslint": "^5.16.0",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-vue": "^5.0.0",
    "mocha": "^6.2.2",
    "nodemon": "^1.19.4",
    "sass": "^1.19.0",
    "sass-loader": "^8.0.0",
    "vue-cli-plugin-vuetify": "^2.0.2",
    "vue-template-compiler": "^2.6.10",
    "vuetify-loader": "^1.3.0"
  }
}
